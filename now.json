{
  "name": "boxware-install",
  "version": 2,
  "scope": "boxware",
  "github": {
    "enabled": true,
    "silent": true
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "45mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
