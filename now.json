{
  "name": "boxware-install",
  "version": 2,
  "alias": ["get.boxware.now.sh"],
  "github": {
    "enabled": true,
    "silent": true
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
